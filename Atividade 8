algoritmo "Convers찾o de dist창ncia"

var
   distancia_m: real

inicio
   escreva("Digite uma dist창ncia em metros: ")
   leia(distancia_m)
   escreva("A dist창ncia de " + distancia_m + "m corresponde a:")
   escreva(distancia_m / 1000 + "Km")
   escreva(distancia_m / 100 + "Hm")
   escreva(distancia_m / 10 + "Dam")
   escreva(distancia_m * 10 + "dm")
   escreva(distancia_m * 100 + "cm")
   escreva(distancia_m * 1000 + "mm")
fim_algoritmo
