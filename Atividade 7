algoritmo "Dobro e terça parte de um número"

var
   num, dobro, terca_parte: real

inicio
   escreva("Digite um número: ")
   leia(num)
   dobro <- num * 2
   terca_parte <- num / 3
   escreva("O dobro de " + num + " é " + dobro + ".")
   escreva("A terça parte de " + num + " é " + terca_parte + ".")
fim_algoritmo
